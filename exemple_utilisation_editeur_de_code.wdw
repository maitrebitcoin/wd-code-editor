    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
         // decrit les mot du langage pour leur coloration
         // au format "prims"
         // voir https://prismjs.com/extending.html#language-definitions
         Prism_languages est chaine = [
         // commentaire
         'comment': {
         	pattern: /\/\/.*|\/\*[\s\s]*?(?:\*\/|$)/,
         	greedy: true
         },
         // nombre
         'number': /-?\b\d+(?:\.\d+)?(?:e[+-]?\d+)?\b/i,
         // mot clé
         'keyword': /renvoyer/
         ]
         
         // init FI_editeur_de_code 
         param est Champ_editeur_de_code.ParametreEditeur
         param.avec_numéro_de_ligne	= Vrai
         param.nom_langage			= "mon_langage"
         param.definition_token		= Prism_languages
         CHAMP_ED_CODE.init_editeur(param)
         
         Multitâche(-10)
         CHAMP_ED_CODE = "//autre code" + rc + "renvoyer 42"
        type : 18
      -
        type : 161
        enabled : false
  -
    name : Couleurs_personalisées
    identifier : 0x23ba89e603fde9f0
    internal_properties : 
    properties :
     x : 10
     y : 131
     width : 147
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
         // décrit les mot du langage pour leur coloration
         // au format "prims"
         // voir https://prismjs.com/extending.html#language-definitions
         definition_de_mon_langage est chaine = [
         // commentaire
         'comment': {
         	pattern: /\/\/.*|\/\*[\s\s]*?(?:\*\/|$)/,
         	greedy: true
         },
         // nombre
         'number': /-?\b\d+(?:\.\d+)?(?:e[+-]?\d+)?\b/i,
         // mot clé
         'keyword': /renvoyer/
         ]
         // style css a applique aux mot du langage
         // voir https://github.com/kazzkiq/CodeFlask/blob/master/src/styles/theme-default.js
         style_css_de_mon_langage est chaine = [
         .codeflask  {
           background: #FFFFFA;
         }
         .codeflask .token.comment {
           //color: #9badb7;
           background: #9badb7;
           width:400px;
           display:inline-block;
         }
         .codeflask .token.keyword {
           color: blue;
           font-weight: bold;
         }
         ]
         
         
         // init FI_editeur_de_code 
         param est Champ_editeur_de_code.ParametreEditeur
         param.avec_numéro_de_ligne	= Vrai
         param.nom_langage			= "mon_langage2"
         param.definition_token		= definition_de_mon_langage
         param.style_css_token		= style_css_de_mon_langage
         CHAMP_ED_CODE.init_editeur(param)
         
         Multitâche(-10)
         CHAMP_ED_CODE = "// style personnalisé" + rc + "renvoyer 42"
        type : 18
      -
        type : 161
        enabled : false
